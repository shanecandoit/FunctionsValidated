[run]
source = app
branch = True
omit =
    */tests/*
    */migrations/*
    */site-packages/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == "__main__":
    pass
    raise NotImplemented
    if TYPE_CHECKING:

[html]
directory = coverage_html_report
